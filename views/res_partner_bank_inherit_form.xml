<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="res_partner_bank_inherit_form" model="ir.ui.view">
            <field name="name">res.partner.bank.form.inherit</field>
            <field name="model">res.partner.bank</field>
            <field name="inherit_id" ref="account.view_partner_bank_form_inherit_account" />
            <field name="arch" type="xml">
                <xpath expr="//field[@name='acc_holder_name']" position="after">
                    <field name="is_factoring" invisible="0" />
                </xpath>
            </field>
        </record>
    </data>

</odoo>
