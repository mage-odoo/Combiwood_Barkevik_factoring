<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="view_move_form_inherit_Combiwood_Barkevik_factoring" model="ir.ui.view">
            <field name="name">account.move.view.form.inherit.factoring</field>
            <field name="model">account.move</field>
            <field name="inherit_id" ref="account.view_move_form" />
            <field name="arch" type="xml">
                <xpath expr="//page[@name='other_info']//field[@name='to_check']" position="after">
                    <field name="is_generate_file" />
                    <field name="is_factoring" readonly="1" />
                </xpath>
            </field>
        </record>
    </data>
</odoo>
